{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": "off",
        "quotes": [2, "single", {"allowTemplateLiterals": true}],
        "semi": ["off"],
        "arrow-parens": [2, "as-needed", {"requireForBlockBody": true}]
    }
}